{
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "cSpell.words": [
    "Autoincrement",
    "Bootcamp",
    "Cinics",
    "Corrigido",
    "estrutura",
    "Hellow",
    "nascimento",
    "quiser",
    "vercel"
  ]
}
